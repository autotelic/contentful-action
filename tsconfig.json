{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"alwaysStrict": true,
		"declaration": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"importHelpers": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": false,
		"module": "commonjs",
		"allowJs": true,
		"outDir": "dist",
		"target": "es2015",
		"noImplicitAny": false,
		"noEmit": true,
		"noImplicitReturns": false,
		"noFallthroughCasesInSwitch": true,
		"lib": [
			"es2015",
			"dom"
		],
		"moduleResolution": "node"
	}
}
